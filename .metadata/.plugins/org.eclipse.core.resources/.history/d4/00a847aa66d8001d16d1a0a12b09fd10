create table TB_DOM_FILO(
	ID BIGSERIAL NOT NULL, 
	DESCRICAO VARCHAR(50)  NOT NULL UNIQUE,
	FOLHA_VERDADEIRA BOOL NOT NULL,
    CAULE_VERDADEIRO BOOL  NOT NULL,
    RAIZ_VERDADEIRA BOOL  NOT NULL,
    FRUTO BOOL  NOT NULL,
    FLOR BOOL  NOT NULL,
    SEMENTE BOOL  NOT NULL,
    ATIVO BOOL  NOT NULL,
	CONSTRAINT FILO_PK PRIMARY KEY (ID)
);
create table TB_PLANTA(
	ID BIGSERIAL NOT NULL,
	NOME VARCHAR(50) NOT NULL UNIQUE,
	COR VARCHAR(50) NOT NULL,
	FK_FILO INTEGER REFERENCES TB_DOM_FILO(ID) NOT NULL,
	FK_CLASSE INTEGER REFERENCES TB_DOM_CLASSE(ID) NOT NULL,
	FK_ORDEM INTEGER REFERENCES TB_DOM_ORDEM(ID)NOT NULL,
	FK_RAIZ INTEGER REFERENCES TB_DOM_RAIZ(ID) NOT NULL,
	FK_CAULE INTEGER REFERENCES TB_DOM_CAULE(ID) NOT NULL,
	FK_FOLHAGEM INTEGER REFERENCES TB_DOM_FOLHAGEM(ID) NOT NULL,
	ATIVO BOOL NOT NULL,
	CONSTRAINT PLANTA_PK PRIMARY KEY (ID)
);